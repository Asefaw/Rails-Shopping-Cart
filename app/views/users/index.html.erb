<h1>List of Users</h1>

<table>
  <thead>
    <tr>
      <th>User Name</th>
      <th>Date Created</th> 
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <%if current_user %>
          <%if current_user.email = "admin@admin.com" %>
            <td><%= user.email %></td>
            <td><%= user.remember_created_at %></td>  
          <%end%>
        <%end%>
      </tr>
    <% end %>
  </tbody>
</table>
